<div class="container">
  <h1 class="text-center usercard">Edit Entry</h1>
  <%= form_for @entry, url: city_entry_path(@entry.city, @entry) do |f| %>
    <%= f.label(:title, "Title") %>
    <%= f.text_field :title, class: "form-control", placeholder: "Title" %>
    <br>
      <%= f.label(:body, "Body") %>
    <%= f.text_area :body, class: "form-control", placeholder: "Body" %>
    <br>
    <%= f.submit "Save Entry", class: "btn btn-block btn-default reallyhalf" %>
  <% end %>
</div>
  </div>