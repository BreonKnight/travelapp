<h1><%= @city.name %></h1>
<hr>

<%= image_tag @city.img_url, size: "320x426" %>
<br>
<br>
<%= link_to "Add Entry", new_city_entry_path(@city), class: "btn btn-default" %>
<br>
<br>



<% @entries.reverse.each do |entry| %>
		<div>
	  	<%= link_to entry.title, city_entry_path(entry.city_id, entry) %>
	  	<br>
	  </div>
<% end %>